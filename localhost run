const http = require('http'); const hostname = '127.0.0.1'; const port = 8000; const server = http.createServer((req, res) => { res.statusCode = 200; res.setHeader('Content-Type', 'text/plain'); res.end('Hello World'); }); server.listen(port, hostname, () => { console.log(`Server running at http://${127.0.0.1}:${8080}/`); });

